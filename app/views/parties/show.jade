
//- # party - show

extends ../layout

block content
  .container
    .row
      table
        tbody
          tr
            th Party Title:
            td= party.name
          tr
            th Party Owner:
            td= party.user.email
      form(action='https://api.spotify.com/v1/search?q=', method='GET')
        input(type='text', name='q')
        button(type='submit').btn.btn-lg.btn-primary

